        <SqlCommand>SELECT NHOM
	,tennhom_thebh
	,COUNT(SOTHE) as SLTHE
	,SUM(SONGAYDT) as SONGAYDT
	,SUM(T_TONGCHI) as T_TONGCHI
	,SUM(T_XN) as T_XN
	,SUM(T_CDHA) as T_CDHA
	,SUM(T_THUOC) as T_THUOC
	,SUM(T_THUOC_TYLE) as T_THUOC_TYLE
	,SUM(T_MAU) as T_MAU
	,SUM(T_PTTT) as T_PTTT
	,SUM(T_VTYT) as T_VTYT
	,SUM(T_VTYT_TYLE) as T_VTYT_TYLE
	,SUM(T_KHAM) as T_KHAM
	,SUM(T_GIUONG) as T_GIUONG
	,SUM(T_VANCHUYEN) as T_VANCHUYEN
	,SUM(T_BNTT) as T_BNTT
	,SUM(T_BHTT) as T_BHTT
	,SUM(T_NGOAIDS) as T_NGOAIDS FROM(
SELECT A.ID, to_char(A.ngayvao,'dd/mm/yyyy') AS NGAYVAO, to_char(A.ngayra,'dd/mm/yyyy') AS NGAYRA, A.MABN, D.HOTEN, D.NAMSINH, L.SOTHE, L.TUNGAY, L.DENNGAY, (CASE 
      WHEN (INSTR(A.MAICD, ';')-1)&gt;0 THEN SUBSTR(A.MAICD, 0, INSTR(A.MAICD, ';')-1)
      WHEN (INSTR(A.MAICD, ';')-1)&lt;=0 THEN A.MAICD
   END) AS MAICD, (CASE 
            WHEN A.TRAITUYEN=0 THEN 'Đúng Tuyến'
            WHEN A.TRAITUYEN=1 THEN 'Trái Tuyến'
         END) AS TUYEN, D.PHAI, A.LOAIBA, L.NOICAP,(CASE 
               WHEN a.ngayra-a.ngayvao&lt;=0.35 AND A.LOAIBA=1 THEN 1 
                WHEN a.ngayra-a.ngayvao&gt;0.35 AND A.LOAIBA=1 THEN to_date(a.ngayra)-to_date(a.ngayvao)+1
                WHEN A.LOAIBA!=1 THEN 1
                END
            ) AS SONGAYDT,ROUND(Sum(B.SOTIEN)) AS T_TONGCHI,ROUND(SUM(CASE 
      WHEN C.NHOMVP=14 THEN B.SOTIEN
   END)) AS  T_XN,ROUND(SUM(CASE 
      WHEN C.NHOMVP=17 THEN B.SOTIEN
      WHEN C.NHOMVP=6 THEN B.SOTIEN
   END)) AS  T_CDHA,ROUND(SUM(CASE 
      WHEN C.NHOMVP=15 THEN B.SOTIEN
   END)) AS  T_THUOC,ROUND(SUM(CASE 
      WHEN C.NHOMVP=3 THEN B.SOTIEN
   END)) AS  T_MAU,ROUND(SUM(CASE 
      WHEN C.NHOMVP=9 THEN B.SOTIEN
   END)) AS  T_PTTT,ROUND(SUM(CASE 
      WHEN C.NHOMVP=4 THEN B.SOTIEN
   END)) AS  T_VTYT,Sum(0) AS T_DVKT_TYLE,Sum(0) AS T_THUOC_TYLE, ROUND(SUM(CASE 
