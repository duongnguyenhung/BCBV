WHERE to_date(to_char(a.ngayqt,'dd/mm/yyyy'),'dd/mm/yyyy') between to_date('01/01/2018','dd/mm/yyyy') and to_date('31/01/2018','dd/mm/yyyy') and a.useridduyet IS NOT null AND A.LOAIBA=1
GROUP BY A.ID, A.NGAYVAO, A.NGAYRA, A.MABN, D.HOTEN, D.NAMSINH, L.SOTHE, L.TUNGAY, L.DENNGAY, (CASE 
