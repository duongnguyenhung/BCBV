   WHEN LENGTH(L.SOTHE)&gt;20 AND SUBSTR(L.SOTHE,25,2) !='94' THEN 'C. BỆNH NHÂN NGOẠI TỈNH ĐẾN'
   WHEN LENGTH(L.SOTHE)&gt;20 AND SUBSTR(L.SOTHE,37,5) ='94001' AND SUBSTR(L.SOTHE,25,2) ='94' THEN 'A. BỆNH NHÂN NỘI TỈNH KCB BAN ĐẦU'
   WHEN LENGTH(L.SOTHE)&gt;20 AND SUBSTR(L.SOTHE,25,2) ='94' THEN 'B. BỆNH NHÂN NỘI TỈNH ĐẾN'
   END) AS NHOM,(case
      when LENGTH(L.SOTHE)=20 AND instr('DN,HX,CH,NN,TK,HC,XK', substr(L.sothe,1,2))&lt;&gt;0 then '1.Nhóm I'
      when LENGTH(L.SOTHE)=20 AND instr('HT,TB,NO,CT,XB,TN,CS', substr(L.sothe,1,2))&lt;&gt;0 then '2.Nhóm II'
      when LENGTH(L.SOTHE)=20 AND instr('QN,CA,CY,XN,MS,CC,CK,HD,TE,BT,HN,DT,DK,XD,TS,TC,TQ,TA,TY,HG,LS,KC,CB,TV', substr(L.sothe,1,2))&lt;&gt;0 then '3.Nhóm III'
      when LENGTH(L.SOTHE)=20 AND instr('CN,HS,SV,GB', substr(L.sothe,1,2))&lt;&gt;0 then '4.Nhóm IV'
      when LENGTH(L.SOTHE)=20 AND instr('GD,PV', substr(L.sothe,1,2))&lt;&gt;0 then '5.Nhóm V'
	  when LENGTH(L.SOTHE)&gt;20 AND instr('DN,HX,CH,NN,TK,HC,XK', substr(L.sothe,22,2))&lt;&gt;0 then '1.Nhóm I'
      when LENGTH(L.SOTHE)&gt;20 AND instr('HT,TB,NO,CT,XB,TN,CS', substr(L.sothe,22,2))&lt;&gt;0 then '2.Nhóm II'
