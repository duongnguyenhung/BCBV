            WHEN A.TRAITUYEN=1 THEN 'Trái Tuyến'
         END), D.PHAI, A.LOAIBA, L.NOICAP, (CASE 
                WHEN a.ngayra-a.ngayvao&lt;=0.35 AND A.LOAIBA=1 THEN 1 
                WHEN a.ngayra-a.ngayvao&gt;0.35 AND A.LOAIBA=1 THEN to_date(a.ngayra)-to_date(a.ngayvao)+1
                WHEN A.LOAIBA!=1 THEN 1
                END),(CASE 
	WHEN LENGTH(L.SOTHE)=20 AND SUBSTR(L.SOTHE,4,2) !='94' THEN 'C. BỆNH NHÂN NGOẠI TỈNH ĐẾN'
   WHEN LENGTH(L.SOTHE)=20 AND SUBSTR(L.SOTHE,16,5) ='94001' AND SUBSTR(L.SOTHE,4,2) ='94' THEN 'A. BỆNH NHÂN NỘI TỈNH KCB BAN ĐẦU'
   WHEN LENGTH(L.SOTHE)=20 AND SUBSTR(L.SOTHE,4,2) ='94' THEN 'B. BỆNH NHÂN NỘI TỈNH ĐẾN' 
   WHEN LENGTH(L.SOTHE)&gt;20 AND SUBSTR(L.SOTHE,25,2) !='94' THEN 'C. BỆNH NHÂN NGOẠI TỈNH ĐẾN'
   WHEN LENGTH(L.SOTHE)&gt;20 AND SUBSTR(L.SOTHE,37,5) ='94001' AND SUBSTR(L.SOTHE,25,2) ='94' THEN 'A. BỆNH NHÂN NỘI TỈNH KCB BAN ĐẦU'
   WHEN LENGTH(L.SOTHE)&gt;20 AND SUBSTR(L.SOTHE,25,2) ='94' THEN 'B. BỆNH NHÂN NỘI TỈNH ĐẾN'
   END),(case
      when LENGTH(L.SOTHE)=20 AND instr('DN,HX,CH,NN,TK,HC,XK', substr(L.sothe,1,2))&lt;&gt;0 then '1.Nhóm I'
      when LENGTH(L.SOTHE)=20 AND instr('HT,TB,NO,CT,XB,TN,CS', substr(L.sothe,1,2))&lt;&gt;0 then '2.Nhóm II'
      when LENGTH(L.SOTHE)=20 AND instr('QN,CA,CY,XN,MS,CC,CK,HD,TE,BT,HN,DT,DK,XD,TS,TC,TQ,TA,TY,HG,LS,KC,CB,TV', substr(L.sothe,1,2))&lt;&gt;0 then '3.Nhóm III'
      when LENGTH(L.SOTHE)=20 AND instr('CN,HS,SV,GB', substr(L.sothe,1,2))&lt;&gt;0 then '4.Nhóm IV'
      when LENGTH(L.SOTHE)=20 AND instr('GD,PV', substr(L.sothe,1,2))&lt;&gt;0 then '5.Nhóm V'
	  when LENGTH(L.SOTHE)&gt;20 AND instr('DN,HX,CH,NN,TK,HC,XK', substr(L.sothe,22,2))&lt;&gt;0 then '1.Nhóm I'
      when LENGTH(L.SOTHE)&gt;20 AND instr('HT,TB,NO,CT,XB,TN,CS', substr(L.sothe,22,2))&lt;&gt;0 then '2.Nhóm II'
      when LENGTH(L.SOTHE)&gt;20 AND instr('QN,CA,CY,XN,MS,CC,CK,HD,TE,BT,HN,DT,DK,XD,TS,TC,TQ,TA,TY,HG,LS,KC,CB,TV', substr(L.sothe,22,2))&lt;&gt;0 then '3.Nhóm III'
      when LENGTH(L.SOTHE)&gt;20 AND instr('CN,HS,SV,GB', substr(L.sothe,22,2))&lt;&gt;0 then '4.Nhóm IV'
      when LENGTH(L.SOTHE)&gt;20 AND instr('GD,PV', substr(L.sothe,22,2))&lt;&gt;0 then '5.Nhóm V'
      end )
UNION ALL
SELECT A.ID, to_char(A.ngayvao,'dd/mm/yyyy') AS NGAYVAO, to_char(A.ngayra,'dd/mm/yyyy') AS NGAYRA, A.MABN, D.HOTEN, D.NAMSINH, L.SOTHE, L.TUNGAY, L.DENNGAY, (CASE 
      WHEN (INSTR(A.MAICD, ';')-1)&gt;0 THEN SUBSTR(A.MAICD, 0, INSTR(A.MAICD, ';')-1)
      WHEN (INSTR(A.MAICD, ';')-1)&lt;=0 THEN A.MAICD
   END) AS MAICD, (CASE 
            WHEN A.TRAITUYEN=0 THEN 'Đúng Tuyến'
            WHEN A.TRAITUYEN=1 THEN 'Trái Tuyến'
         END) AS TUYEN, D.PHAI, A.LOAIBA, L.NOICAP,(CASE 
               WHEN a.ngayra-a.ngayvao&lt;=0.35 AND A.LOAIBA=1 THEN 1 
                WHEN a.ngayra-a.ngayvao&gt;0.35 AND A.LOAIBA=1 THEN to_date(a.ngayra)-to_date(a.ngayvao)+1
