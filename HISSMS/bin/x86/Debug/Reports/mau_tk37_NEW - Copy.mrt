        <Name>DataSource1</Name>
        <NameInSource>Oracle</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT c.id
	,c.ten as ten
	,sum(b.soluong) as soluong
	,b.dongia as dongiat7
	,sum(b.soluong*b.dongia) as tongtient7
	,d.ten as ten_loai
	,e.ten as ten_nhom
	,a.loaiba
	,c.QD366
	,a.SOTHE
	,SUBSTR(b.SOTHE,4,2) as NOISINHSONG
	,B.NOICAP
	,(CASE 
            WHEN B.DONGIA=10500 AND E.MA=13 THEN 4500
            ELSE c.gia_1
            END)
      AS DONGIA3    
      FROM HSOFT1117.v_m38ll a join HSOFT1117.v_m38ct b on (a.id=b.id)
           join HSOFT.v_giavp c on (b.mavp = c.id )
           join HSOFT.v_loaivp d on (c.id_loai = d.id )
           join HSOFT.v_nhomvp e on (d.id_nhom = e.ma )
      where to_date(to_char(a.ngayqt,'dd/mm/yyyy'),'dd/mm/yyyy') between to_date('01/11/2017','dd/mm/yyyy') and to_date('30/11/2017','dd/mm/yyyy') and a.useridduyet IS NOT NULL and( e.ma=13 or e.ma=14 or e.ma=11 or e.ma=17 or e.ma=6 or e.ma=9 or e.ma=2 or e.ma=16) and a.loaiba=1
      GROUP BY c.id,c.ten,c.gia_1,b.dongia,d.ten,e.ten,a.loaiba,c.QD366,a.SOTHE,SUBSTR(b.SOTHE,4,2),B.NOICAP,CASE
      WHEN B.DONGIA=10500 AND E.MA=13 THEN 4500 ELSE c.gia_1 END
union all
SELECT c.id
	,c.ten as ten
	,sum(b.soluong) as soluong
	,b.dongia as dongiat7
	,sum(b.soluong*b.dongia) as tongtient7
	,d.ten as ten_loai
	,e.ten as ten_nhom
	,a.loaiba
	,c.QD366
	,a.SOTHE
	,SUBSTR(b.SOTHE,4,2) as NOISINHSONG
	,B.NOICAP
	,(CASE 
            WHEN B.DONGIA=10500 AND E.MA=13 THEN 4500
            ELSE c.gia_1
            END)
      AS DONGIA3    
      FROM HSOFT1217.v_m38ll a join HSOFT1217.v_m38ct b on (a.id=b.id)
           join HSOFT.v_giavp c on (b.mavp = c.id )
           join HSOFT.v_loaivp d on (c.id_loai = d.id )
           join HSOFT.v_nhomvp e on (d.id_nhom = e.ma )
      where to_date(to_char(a.ngayqt,'dd/mm/yyyy'),'dd/mm/yyyy') between to_date('01/11/2017','dd/mm/yyyy') and to_date('30/11/2017','dd/mm/yyyy') and a.useridduyet IS NOT NULL and( e.ma=13 or e.ma=14 or e.ma=11 or e.ma=17 or e.ma=6 or e.ma=9 or e.ma=2 or e.ma=16) and a.loaiba=1
      GROUP BY c.id,c.ten,c.gia_1,b.dongia,d.ten,e.ten,a.loaiba,c.QD366,a.SOTHE,SUBSTR(b.SOTHE,4,2),B.NOICAP,CASE
      WHEN B.DONGIA=10500 AND E.MA=13 THEN 4500 ELSE c.gia_1 END</SqlCommand>
      </DataSource1>
